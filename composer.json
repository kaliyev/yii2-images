{
    "name": "kaliyev/yii2-images",
    "description": "Yii2 images module for storing images",
    "keywords": ["yii2", "images", "store", "images store", "yii2-images"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Rustembek Kaliyev",
            "email": "kaliyev.r@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "*",
        "abeautifulsite/simpleimage": "2.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "suggest": {
        "ext-imagick": ""
    },
    "autoload": {
        "psr-4": {
            "kaliyev\\yii2images\\": ""
        }
    }
}
